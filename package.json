{
  "name": "@ushironoko/gistdex",
  "version": "1.5.3",
  "description": "RAG search system for Gist, GitHub, and Local Files.",
  "type": "module",
  "main": "dist/index.js",
  "bin": {
    "gistdex": "dist/cli/index.js"
  },
  "exports": {
    ".": {
      "types": "./dist/index.d.ts",
      "import": "./dist/index.js"
    },
    "./package.json": "./package.json"
  },
  "files": [
    "dist",
    "README.md",
    "LICENSE",
    "CHANGELOG.md",
    "templates",
    "package.json"
  ],
  "scripts": {
    "dev": "tsdown --watch --tsconfig tsconfig.app.json",
    "build": "tsdown --tsconfig tsconfig.app.json",
    "start": "node dist/cli/index.js",
    "test": "bun test src/**/*.test.ts && vitest run --config vitest.config.unit.ts",
    "test:integration": "pnpm run build && vitest run --config vitest.config.integration.ts && tsx scripts/cleanup-test-dirs.ts",
    "test:all": "pnpm test && pnpm run test:integration",
    "lint": "biome check --write .",
    "format": "biome format --write .",
    "tsc": "tsgo --project tsconfig.app.json --noEmit && tsgo --project tsconfig.test.json --noEmit",
    "run-all": "pnpm format && pnpm run lint && pnpm run tsc && pnpm run test:unit && pnpm run build && pnpm run docs:build",
    "docs:dev": "vitepress dev docs",
    "docs:build": "vitepress build docs",
    "docs:preview": "vitepress preview docs"
  },
  "keywords": [
    "rag",
    "vector-search",
    "embeddings",
    "gist",
    "github",
    "cli",
    "ai",
    "llm"
  ],
  "author": "ushironoko",
  "license": "MIT",
  "repository": {
    "type": "git",
    "url": "git+https://github.com/ushironoko/gistdex.git"
  },
  "bugs": {
    "url": "https://github.com/ushironoko/gistdex/issues"
  },
  "homepage": "https://github.com/ushironoko/gistdex#readme",
  "publishConfig": {
    "access": "public",
    "registry": "https://registry.npmjs.org/"
  },
  "dependencies": {
    "@ai-sdk/google": "2.0.13",
    "@inquirer/prompts": "7.8.4",
    "@modelcontextprotocol/sdk": "1.17.5",
    "ai": "5.0.38",
    "consola": "3.2.3",
    "defu": "6.1.4",
    "gunshi": "0.26.3",
    "picomatch": "4.0.3",
    "sqlite-vec": "0.1.6",
    "tree-sitter-wasms": "0.1.11",
    "web-tree-sitter": "0.24.5",
    "zod": "3.25.76"
  },
  "devDependencies": {
    "@biomejs/biome": "2.2.3",
    "@iconify-json/vscode-icons": "1.2.30",
    "@iconify/json": "2.2.382",
    "@types/bun": "1.2.21",
    "@types/node": "24.3.1",
    "@types/picomatch": "4.0.2",
    "@typescript/native-preview": "7.0.0-dev.20250808.1",
    "@vitest/coverage-v8": "3.2.4",
    "tsdown": "0.14.2",
    "tsx": "4.20.5",
    "typescript": "5.9.2",
    "vitepress": "2.0.0-alpha.12",
    "vitepress-plugin-group-icons": "1.6.3",
    "vitepress-plugin-llms": "1.7.4",
    "vitest": "3.2.4"
  },
  "packageManager": "pnpm@10.15.0"
}
